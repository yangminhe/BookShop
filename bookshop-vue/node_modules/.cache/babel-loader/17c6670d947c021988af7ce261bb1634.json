},\n    handleCurrentChange(val) {\n      this.pageNum = val;\n      this.list(val);\n    },\n    handleSkip(id) {\n      this.$router.push({\n        path: \"/product\",\n        query: {\n          id: id,\n        },\n      });\n    },\n  },\n};\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":"src/fore"}]}