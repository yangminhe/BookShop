AAA,QAAA,CAAA,IAAA;AACA,YAAA,GAAA,GAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,YAAA,KAAA,GAAA,MAAA,CAAA,IAAA,CAAA,KAAA;;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,KADA;AAEA,UAAA,KAAA,EAAA;AACA,YAAA,GAAA,EAAA,GADA;AAEA,YAAA,KAAA,EAAA;AAFA;AAFA,SAAA;AAOA,OAZA;AAaA;AAhBA;AA5BA,CAAA","sourcesContent":["<template>\n    <div>\n        <el-row style=\"margin-left:20%;margin-right: 20%;\">\n            <h3>确认订单信息</h3>\n            <el-card style=\"width: 300px\">\n                <el-form ref=\"form\" :model=\"form\" size=\"mini\">\n                    <el-form-item label=\"地址\">\n                        <el-input v-model=\"form.address\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"收件人姓名\">\n                        <el-input v-model=\"form.recipient\"></el-input>\n                    </el-form-item>\n                    <el-form-item label=\"联系方式\">\n                        <el-input v-model=\"form.tel\"></el-input>\n                    </el-form-item>\n                </el-form>\n            </el-card>\n        </el-row>\n        <el-row style=\"margin-left: 20%;margin-right: 20%;margin-top: 30px\">\n        <el-table :data=\"orderItems\"\n                  border>\n            <el-table-column prop=\"product.name\"\n                             label=\"商品信息\"/>\n            <el-table-column prop=\"product.price\"\n                             label=\"单价\"/>\n            <el-table-column prop=\"number\"\n                             label=\"数量\"/>\n        </el-table>\n        </el-row>\n        <el-row style=\"margin-left: 20%;margin-right: 20%;margin-top: 30px;text-align: right\">\n            <p>应付金额：</p>\n            <h2 style=\"color: #fb4e44\">{{total}}</h2>\n            <el-button type=\"danger\" @click=\"handleSubmit\">提交订单</el-button>\n        </el-row>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"Confirm\",\n        data(){\n            return{\n                uri:'confirm',\n                total:0,\n                oid:'',\n                orderItems:[],\n                product:{},\n                form:{\n                    address:'',\n                    recipient:'',\n                    tel:'',\n                },\n                user:'',\n            }\n        },\n        created() {\n            let id = this.$route.query.oiid;\n            let url = this.uri + \"/?oiid=\" + id;\n            this.axios.get(url)\n                .then(response => {\n                        let result = response.data;\n                        this.total = result.data.total;\n                        this.orderItems = result.data.orderItems;\n                    }\n                )\n        },\n        methods:{\n            handleSubmit(){\n                let url = '/createOrder';\n                this.axios.post(url,this.form)\n                    .then(response => {\n                        let result = response.data;\n                        let oid = result.data.oid;\n                        let total = result.data.total;\n                        this.$router.push({\n                            path: 'pay',\n                            query: {\n                                oid : oid,\n                                total : total\n                            }\n                        })\n                    })\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/fore"}]}