               <el-image :src=\"item.image\"\n                                  style=\"height: 276px;width: 195px\"\n                        />\n                        <div style=\"text-align: center\">\n                            <p style=\"font-size: 5px\">{{item.name}}</p>\n                            <p style=\"color: #9d9d9d;font-size: 3px\">{{item.author}}</p>\n                            <p style=\"color:#fb4e44;font-size: 5px\">¥{{item.price}}</p>\n                        </div>\n                    </el-card>\n                </el-col>\n            </el-row>\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"productPage\",\n        data(){\n            return{\n                uri:'search',\n                beans:[],\n            }\n        },\n        created() {\n            this.refresh();\n        },\n        watch: {\n            '$route'() {\n                this.refresh();\n            }\n        },\n        methods: {\n            refresh() {\n                let keyword = this.$route.query.keyword;\n                let url = this.uri + \"?keyword=\" + keyword;\n                this.axios.post(url)\n                    .then(response => {\n                        this.beans = response.data;\n                    })\n            },\n            handleSkip(id) {\n                this.$router.push({\n                    path: '/product',\n                    query: {\n                        id: id\n                    }\n                })\n            },\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}