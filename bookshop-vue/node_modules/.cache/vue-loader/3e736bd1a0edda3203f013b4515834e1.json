               <span>{{scope.row.dealTime}}</span>\n                        </el-form-item>\n                    </el-form>\n                </template>\n            </el-table-column>\n            <el-table-column\n                    prop=\"id\"\n                    label=\"订单ID\"/>\n            <el-table-column\n                    prop=\"statusDesc\"\n                    label=\"状态\"/>\n            <el-table-column\n                    prop=\"amount\"\n                    label=\"金额\"/>\n            <el-table-column\n                    prop=\"sum\"\n                    label=\"商品数量\"/>\n            <el-table-column\n                    prop=\"recipient\"\n                    label=\"收件人\"/>\n            <el-table-column\n                    label=\"操作\">\n                <template slot-scope=\"scope\">\n                    <el-button\n                            type=\"danger\"\n                            icon=\"el-icon-position\"\n                            v-show=\"scope.row.status === 'waitDelivery'\"\n                            @click=handleDeliver(scope.$index,scope.row)>发货\n                    </el-button>\n                </template>\n            </el-table-column>\n        </el-table>\n        <el-col :span=\"24\">\n            <el-pagination\n                    background\n                    layout=\"total,prev,pager,next\"\n                    :page-size=\"5\"\n                    :pager-count=\"5\"\n                    :hide-on-single-page=\"value\"\n                    @current-change=\"handleCurrentChange\"\n                    :total=\"total\"\n                    :current-page=\"pageNum\"\n            />\n        </el-col>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"Order\",\n        data() {\n            return {\n                uri: 'orders',\n                total: 0,\n                pageNum: 0,\n                bean: [],\n            }\n        },\n        mounted() {\n            this.list(1);\n        },\n        methods: {\n            list(pageNum) {\n                let url = this.uri + \"?pageNum=\" + pageNum;\n                this.axios.get(url)\n                    .then(response => {\n                        this.bean = response.data.content;\n                        this.total = response.data.totalElements;\n                    })\n            },\n            handleCurrentChange(val) {\n                this.pageNum = val;\n                this.list(val);\n            },\n            handleDeliver($index,row){\n                let url = this.uri + \"/\" + row.id;\n                this.axios.put(url)\n                    .then(response => {\n                        if(0 != response.data.length) {\n                            this.handleCurrentChange(1);\n                        }\n                    })\n            },\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}